      <p><%= userDB.bio %></p>
      
      <% if (userDB !== "" && userDB.facebook !== "") { %>
      <a href="<%= userDB.facebook %>"><i class="fab fa-facebook-square"></i></a>
    <% } %>
    <% if (userDB !== "" && userDB.twitter !== "") { %>
      <a href="<%= userDB.twitter %>"><i class="fab fa-twitter"></i></a>
