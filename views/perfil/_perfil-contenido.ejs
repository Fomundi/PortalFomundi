    <% } %>
    <% if (userDB !== "" && userDB.web !== "") { %>
      <a href="<%= userDB.web %>"><i class="fab fa-chrome"></i></a>
    <% } %>
    <% if (userDB !== "" && userDB.sdiscord !== "") { %>
      <a href="<%= userDB.sdiscord %>"><i class="fab fa-discord"></i></a>
    <% } %>
    
    <p><span class="icon icon-location2"></span>        <%= userDB.pais_ciudad %></p>
    
