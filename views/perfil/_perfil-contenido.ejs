  <div class="puser">
  <% if(user.avatar) { %>
    <img class="img-fluid rounded-circle" src="https://cdn.discordapp.com/avatars/<%= user.id %>/<%= user.avatar %>.png"><span class="nivel badge rounded-pill bg-secondary text-center"><p class="textnivel"><%= userDB.nivel %></p></span>
    
  <% } else { %> 
    <img class="img-fluid rounded-circle" src="https://images-ext-1.discordapp.net/external/NAqkMZNPJgDiWBrSDqniAD1_sbWfiPqF4mgZyCtVs6s/https/discordapp.com/assets/6debd47ed13483642cf09e832ed0bc1b.png?width=231&height=231" width="200px" height="200px" alt=""><span class="nivel badge rounded-pill bg-secondary text-center"><p class="textnivel"><%= userDB.nivel %></p></span>
 <% } %>
